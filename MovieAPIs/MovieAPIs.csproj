<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework Condition="'$(TargetFramework)' == 'net5'">net5.0</TargetFramework>
	<TargetFramework Condition="'$(TargetFramework)' != 'net5'">net6.0</TargetFramework>
    <Nullable>enable</Nullable>
	<NoWarn>CS8618</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Update="Configuration\configuration.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
